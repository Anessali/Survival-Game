::Links [widget]
/*  Widget used to exit the menu  */
<<widget "exitMenu">><<nobr>>
    <<link "Return" $exitMenuPassage>>
        <<set $menu = false>>
        <<set $showFooter = true>>
    <</link>>
<</nobr>><</widget>>


<<widget "explore">><<nobr>>
    <<set _explore = explore($biome, $area, $location)>>
    <<link "Explore" _explore>>
        <<set $hours += 1>>
    <</link>>
<</nobr>><</widget>>
